<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="car_gallery.core.base.manager" class="CarGallery\CoreBundle\Manager\BaseManager">
            <argument type="service" id="doctrine.orm.default_entity_manager"/>
        </service>

        <service id="car_gallery.core.driver.manager" class="CarGallery\CoreBundle\Manager\DriverManager">
            <argument type="service" id="car_gallery.core.driver.repository"/>
        </service>
    </services>
